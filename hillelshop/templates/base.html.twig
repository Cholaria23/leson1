<!DOCTYPE html>
<html>

<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
    <link rel="stylesheet" type="text/css" href="web/styles/bootstrap4/bootstrap.min.css">
    <link href="web/plugins/fontawesome-free-5.0.1/css/fontawesome-all.css" rel="stylesheet" type="text/css">
    <link rel="stylesheet" type="text/css" href="web/plugins/OwlCarousel2-2.2.1/owl.carousel.css">
    <link rel="stylesheet" type="text/css" href="web/plugins/OwlCarousel2-2.2.1/owl.theme.default.css">
    <link rel="stylesheet" type="text/css" href="web/plugins/OwlCarousel2-2.2.1/animate.css">
    <link rel="stylesheet" type="text/css" href="web/plugins/slick-1.8.0/slick.css">
    <link rel="stylesheet" type="text/css" href="web/styles/main_styles.css">
    <link rel="stylesheet" type="text/css" href="web/styles/responsive.css"> 
{% endblock %}
</head>

<body>
    <div class="super_container">
	 <!-- Body -->
	    {% block body %}
			   
			 <!-- Header -->
		    {% block header %} 
		        {% include 'base/header.html.twig' %}
		    {% endblock %} 

		    <!-- Baner -->
		    {% block baner %}
			    {% include "base/partial/baner.html.twig" %}
			{% endblock %}

			<!-- Characteristics -->
			{% block characteristics  %}
				{% include "base/partial/characteristics.html.twig" %}
			{% endblock %}
			<!-- Deals of the week -->
		    {% block deals_of_the_week %}
		        {% include 'base/partial/deals_of_the_week.html.twig' %}
		    {% endblock %}

			<!-- Popular Categories -->
		    {% block popular_categories %}
		        {% include 'base/partial/popular_categories.html.twig' %}    
		    {% endblock %}    
			
			<!-- Baner_2-->
			{% block banner_2 %}  
				{% include  "base/partial/baner_2.html.twig" %}
			{% endblock %}
			
			<!-- Hot New Arrivals -->
		    {% block new_arrivals %}
		        {% include 'base/partial/new_arrivals.html.twig' %}
		    {% endblock %}

		    <!-- Best Sellers -->
		    {% block best_sellers %}
		        {% include 'base/partial/best_sellers.html.twig' %}
		    {% endblock %}

				<!-- Adverts -->
		    {% block adverts %}
		        {% include 'base/partial/adverts.html.twig' %}    
		    {% endblock %}

				<!-- Trends -->
		    {% block trends %}
		        {% include 'base/partial/trends.html.twig' %}
		    {% endblock %}

				<!-- Reviews -->
		    {% block reviews %}
		        {% include 'base/partial/reviews.html.twig' %}
		    {% endblock %}

				<!-- Recently Viewed -->
		    {% block viewed %}
		        {% include 'base/partial/viewed.html.twig' %}
		    {% endblock %}

				<!-- Brands -->
		    {% block brands %}
		       {% include 'base/partial/brands.html.twig' %} 
		    {% endblock %}

				<!-- Newsletter -->
		    {% block newsletter %}    
		        {% include 'base/partial/newsletter.html.twig' %}
		    {% endblock %}

				 <!-- Copyright -->
		    {% block copyright %}
		        {% include 'base/partial/copyrigth.html.twig' %}
		    {% endblock %}

			<!--Footer-->
			{% block footer %}
				{% include 'base/footer.html.twig' %}
			{% endblock %}
		{% endblock %} 
	</div>
    
    {% block javascripts %}
		<script src="web/js/jquery-3.3.1.min.js"></script>
		<script src="web/styles/bootstrap4/popper.js"></script>
		<script src="web/styles/bootstrap4/bootstrap.min.js"></script>
		<script src="web/plugins/greensock/TweenMax.min.js"></script>
		<script src="web/plugins/greensock/TimelineMax.min.js"></script>
		<script src="web/plugins/scrollmagic/ScrollMagic.min.js"></script>
		<script src="web/plugins/greensock/animation.gsap.min.js"></script>
		<script src="web/plugins/greensock/ScrollToPlugin.min.js"></script>
		<script src="web/plugins/OwlCarousel2-2.2.1/owl.carousel.js"></script>
		<script src="web/plugins/slick-1.8.0/slick.js"></script>
		<script src="web/plugins/easing/easing.js"></script>
		<script src="web/js/custom.js"></script>
    {% endblock %}

</body>

</html>